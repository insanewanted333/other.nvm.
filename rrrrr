local Chatit = createnew('TextButton',{
ZIndex = 8;
Text = "Chat";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .83;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(.95,0,0,24);
Font = Enum.Font.SourceSansLight;
Position = UDim2.new(0.025,0,0,90);
Parent = window.Content;
})

Chatit.MouseButton1Click:Connect(function()
msg = FakeMsg.Text
msg = string.gsub(msg,"\n","\r")
if TextChatService.ChatVersion == Enum.ChatVersion.TextChatService then
TextChatService.TextChannels.RBXGeneral:SendAsync(PreTxt.Text..'\r'.. "[" .. PlrName.Text .. "]: " .. msg)
else
ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(PreTxt.Text..string.sub("                                                                                                                                                           ",#PreTxt.Text).. "[" .. PlrName.Text .. "]: " .. msg,"All")
end
end)

local ChatitV = createnew('TextButton',{
ZIndex = 8;
Text = "Chat With Verified Symbol";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
