function ScanG()
if DelScan then

local mobileOffset = 0.25
local rStorage = cloneref(game:GetService("ReplicatedStorage"))
local wordList = {"delete", "remove", "destroy", "clean", "clear","bullet", "bala", "shoot", "shot", "fire", "segway", "handless", "sword", "attack", "despawn", "deletar", "apagar"} 
local bodyColors = LclPlayer.Character:WaitForChild("Body Colors",0.1) or LclPlayer.Character:WaitForChild("Head"):WaitForChild("HatAttachment",0.1) or LclPlayer.Character:WaitForChild("Head")
if not bodyColors then return error("no test error") end

local remotes = {}
local services = {}
local atts = {
	"BodyProportionScale",
	"BodyWidthScale",
	"BodyHeightScale",
	"BodyDepthScale",
	"HeadScale",
	"BodyTypeScale"
}
local limbs = {
	"arm",
	"leg",
	"foot"
}
checkTime = 0.33

if not getgenv().foundRemote then
	for i,service in pairs(game:GetChildren()) do
		if service.ClassName == "ReplicatedStorage" or service.ClassName == "Workspace" then continue end
		table.insert(services, service)
	end

	local function checkRemote(remote)
		local currentBC = bodyColors
		local currentChar = LclPlayer.Character
		remote:FireServer(bodyColors)
		task.wait(mobileOffset + checkTime + (LclPlayer:GetNetworkPing()*2))
		if (#currentChar:GetChildren() < 7) or (#LclPlayer.Character:GetChildren() < 7) or (currentChar ~= LclPlayer.Character) or (currentBC ~= bodyColors) or (bodyColors.Parent == LclPlayer.Character) or (currentBC.Parent == LclPlayer.Character) then return end
		getgenv().foundRemote = remote
delbackdf = true
		return true
	end

	local function scan(instance, softScan)
		checkTime = softScan and 0.75 or 0.5
		for i,v in pairs(instance:GetDescendants()) do
			if getgenv().foundRemote then return end
			if not v:IsA("RemoteEvent") then continue end
			if v:FindFirstChild("__FUNCTION") then continue end
			table.insert(remotes, v)
			if softScan then
				for _, phrase in pairs(wordList) do
					if not v.Name:lower():find(phrase) then continue end
					checkRemote(v)
				end
				continue
			end
			checkRemote(v)
		end
	end
	
	if not getgenv().foundRemote then
		
		scan(rStorage, true)
	end

	if not getgenv().foundRemote then
		
		scan(LclPlayer:FindFirstChildOfClass("PlayerGui"), true)
	end

	if not getgenv().foundRemote then
		
		scan(workspace, true)
	end

	if not getgenv().foundRemote then
		
		scan(rStorage, false)
	end

	if not getgenv().foundRemote then
		
		scan(LclPlayer:FindFirstChildOfClass("PlayerGui"), false)
	end

	if not getgenv().foundRemote then
		
		scan(workspace, false)
	end

	if not getgenv().foundRemote then
		
		for i,v in pairs(services) do
			scan(v, false)
		end
	end
end

if not getgenv().foundRemote then
 warn("no backdoor found")
end
end

-- end of scanner
