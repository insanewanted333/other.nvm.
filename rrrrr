setcmdkey.MouseButton1Click:Connect(function()

local window = Window.new({width = 165, height = 100, name='Set Cmd Key', icon='rbxassetid://17382166939'})
local changebtn = createnew('TextButton',{
ZIndex = 8;
Text = "Set Keybind";
Size = UDim2.new(.45,0,.6,0);
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.05,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})
local crtbtn = createnew('TextLabel',{
ZIndex = 8;
Text = tostring(ToggleKeyCmd):sub(14);
Size = UDim2.new(.45,0,.6,0);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.5,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})

local function v()
local c = UserInputService.InputBegan:wait()
crtbtn.Text = "Press A Key"
		if c.UserInputType == Enum.UserInputType.Keyboard then
		ToggleKeyCmd = tostring(c.KeyCode):sub(14)
		print(tostring(c.KeyCode))
		print(tostring(c.KeyCode):sub(14))
		updatesave()
		wait(0.1)
		ToggleKeyCmd = Enum.KeyCode[ToggleKeyCmd]
crtbtn.Text = tostring(c.KeyCode):sub(14)
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
else
v()
end
end

changebtn.MouseButton1Down:connect(v)
end)

blurButton.MouseButton1Down:connect(function()
local window = Window.new({width = 165, height = 100, name='Set Blur', icon='rbxassetid://951352122'})
local blurLabel = createnew('TextLabel',{
ZIndex = 8;
Text = "Blur Level";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0,60,0,60);
Position = UDim2.new(0.45,-60,0.05,0);
Parent = window.Content;
})
local blurBox  = createnew('TextBox',{
ZIndex = 8;
Text = tostring(blurLevel);
ClearTextOnFocus = false;
TextSize = 28;
MultiLine = false;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0,60,0,60);
Position = UDim2.new(0.55,0,0.05,0);
Parent = window.Content;
})
blurBox.Changed:connect(function()
blurBox.Text = blurBox.Text:gsub("%D","")
end)
blurBox.FocusLost:connect(function(enter)
if enter then
blurLevel = tonumber(blurBox.Text)
blurLevel = KFen and tonumber(blurBox.Text) or blurLevel
KBlurLevel = blurLevel
updatesave()
end
end)
end)

local keepbar = CreateInstance('TextButton',{Font=Enum.Font.SourceSansLight,FontSize=Enum.FontSize.Size24,Text='Keep Command Bar',TextColor3=Color3.fromRGB(255,255,255),TextScaled=false,TextSize=22,TextStrokeColor3=Color3.fromRGB(0, 0, 0),TextStrokeTransparency=1,TextTransparency=0,TextWrapped=true,TextXAlignment=Enum.TextXAlignment.Left,TextYAlignment=Enum.TextYAlignment.Center,AutoButtonColor=true,Modal=false,Selected=false,Style=Enum.ButtonStyle.Custom,Active=true,AnchorPoint=Vector2.new(0, 0),BackgroundColor3=Color3.fromRGB(255,255,255),BackgroundTransparency=1,BorderColor3=Color3.fromRGB(0.105882, 0.164706, 0.207843),BorderSizePixel=0,ClipsDescendants=false,Draggable=false,Position=UDim2.new(0.165000007, 0, 0.122000001, 180),Rotation=0,Selectable=true,Size=UDim2.new(0, 200, 0, 26),SizeConstraint=Enum.SizeConstraint.RelativeXY,Visible=true,ZIndex=8,Name='Keepcmdbar',Parent = windo.Content})

