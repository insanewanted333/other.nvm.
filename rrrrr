tabHolder.About.MouseButton1Click:Connect(function()
local window = Window.new({width = 300, height = 350, name='About', icon='rbxassetid://894507994'})

local Content = window.Content
local Logo = CreateInstance('TextLabel',{Font=Enum.Font.BuilderSansExtraBold,FontSize=Enum.FontSize.Size36,Text='KRUNOX',TextColor3=Color3.fromRGB(255,255,255),TextScaled=false,TextSize=33,TextStrokeColor3=Color3.fromRGB(0, 0, 0),TextStrokeTransparency=1,TextTransparency=0,TextWrapped=false,TextXAlignment=Enum.TextXAlignment.Left,TextYAlignment=Enum.TextYAlignment.Center,Active=false,AnchorPoint=Vector2.new(0, 0),BackgroundColor3=Color3.fromRGB(255,255,255),BackgroundTransparency=1,BorderColor3=Color3.fromRGB(0.105882, 0.164706, 0.207843),BorderSizePixel=1,ClipsDescendants=false,Draggable=false,Position=UDim2.new(0.0500000007, 0, 0.0406249985, 0),Rotation=0,Selectable=false,Size=UDim2.new(0, 200, 0, 50),SizeConstraint=Enum.SizeConstraint.RelativeXY,Visible=true,ZIndex=8,Name='Logo',Parent = Content})
