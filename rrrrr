for i,v in pairs(GetPlayer(args[2])) do
local plr = Players[v]
	
runinst(plr)
end
end)

AddCommand("nora", function()
for i,v in pairs(GetPlayer(args[2])) do
local plr = Players[v]
	
if plr.Character:FindFirstChildOfClass("Humanoid").RigType == Enum.HumanoidRigType.R6 then
			runinst(plr.Character["Right Arm"])
		else
			runinst(plr.Character["RightUpperArm"])
end
end
end)

