-- << Reclone >> --

local datasample = Lib:Clone()
Lib:Destroy()

-- << Functions >> --
function createdatasample(name, type, data1)
if type == "link" then
if SmartLib then
local succ,fail = pcall(game.HttpGetAsync, game, data1)
if (not succ) then
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Invalid URL for script", "Krunox SmartScreen returned \""..fail.."\" for \""..name.."\"");
else
local data = datasample:Clone()
local open = false
data.Parent = Viewer
data.Title.Text = name
data.Type.Text = "Web App"
materializebutton(data.Options.Delete)
materializebutton(data.Options.Execute)
data.Title.MouseButton1Click:Connect(function()
open = not open
data.Options:TweenPosition(UDim2.new(0,0,0,0), "Out", "Quart", .2, true);
end)

data.Options.InputBegan:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 0}):Play()
end
end)

data.Options.InputEnded:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 1}):Play()
end
end)

data.Options.Close.MouseButton1Click:Connect(function()
open = not open
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
end)

data.Options.Execute.MouseButton1Up:Connect(function()
if data.Options.Position.Y.Scale == 0 then
open = not open
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
loadstring(game:HttpGet(data1))()
end
end)

data.Options.Delete.MouseButton1Up:Connect(function()
if data.Options.Position.Y.Scale == 0 then
open = not open
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
for i,v in next, LibScripts do
if v[1] == name then
table.remove(LibScripts, i)
data:Destroy()
Notify({"rbxassetid://1375852124", Color3.fromRGB(0,0,0)}, "Deleted Script", "Script \""..name.."\" has been deleted from your library.")
return
end
end

end
end)

end
else

local data = datasample:Clone()
local open = false
data.Parent = Viewer
data.Title.Text = name
data.Type.Text = "Web App"
materializebutton(data.Options.Delete)
materializebutton(data.Options.Execute)
data.Title.MouseButton1Click:Connect(function()
open = true
data.Options:TweenPosition(UDim2.new(0,0,0,0), "Out", "Quart", .2, true);
end)
data.Options.InputBegan:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 0}):Play()
end
end)

data.Options.InputEnded:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 1}):Play()
end
end)

data.Options.Close.MouseButton1Up:Connect(function()
open = false
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
end)

data.Options.Execute.MouseButton1Up:Connect(function()
if data.Options.Position.Y.Scale == 0 then
open = false
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);

loadstring(game:HttpGet(data1))()
end
end)

data.Options.Delete.MouseButton1Up:Connect(function()
if data.Options.Position.Y.Scale == 0 then
open = not open
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
for i,v in next, LibScripts do
if v[1] == name then
table.remove(LibScripts, i)
data:Destroy()
Notify({"rbxassetid://1375852124", Color3.fromRGB(0,0,0)}, "Deleted Script", "Script \""..name.."\" has been deleted from your library.")
return
end
end

end
end)

end

else
if SmartLib then
local succ, fail = pcall(readfile, data1)
if (not succ) then
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Invalid arguments for script", "Krunox SmartScreen returned\""..fail.."\" for \""..name.."\"");
else
local data = datasample:Clone()
local open = true
data.Parent = Viewer
data.Title.Text = name
data.Type.Text = "Local App"
materializebutton(data.Options.Delete)
materializebutton(data.Options.Execute)
data.Title.MouseButton1Click:connect(function()
open = true
data.Options:TweenPosition(UDim2.new(0,0,0,0), "Out", "Quart", .2, true);
end)

data.Options.InputBegan:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 0}):Play()
end
end)

data.Options.InputEnded:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 1}):Play()
end
end)

data.Options.Close.MouseButton1Up:Connect(function()
open = false
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
end)

data.Options.Execute.MouseButton1Up:Connect(function()
if data.Options.Position.Y.Scale == 0 then
open = false
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
loadstring(data1)()
end
end)

data.Options.Delete.MouseButton1Up:Connect(function()
if data.Options.Position.Y.Scale == 0 then
open = false
data.Options:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quart", .2, true);
for i,v in next, LibScripts do
if v[1] == name then
table.remove(LibScripts, i)
data:Destroy()
Notify({"rbxassetid://1375852124", Color3.fromRGB(0,0,0)}, "Deleted Script", "Script \""..name.."\" has been deleted from your library.")
return
end
end

end
end)

end
else
local data = datasample:Clone()
local open = false
data.Parent = Viewer
data.Title.Text = name
data.Type.Text = "Local App"
materializebutton(data.Options.Delete)
materializebutton(data.Options.Execute)
data.Title.MouseButton1Click:connect(function()
open = true
data.Options:TweenPosition(UDim2.new(0,0,0,0), "Out", "Quart", .2, true);
end)
data.Options.InputBegan:Connect(function(Input)
if Input.UserInputType == Enum.UserInputType.MouseMovement then
TweenService:Create(data.Options.Close, TweenInfo.new(.1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0), {ImageTransparency = 0}):Play()
end
