if enter then

if setfpscap and type(setfpscap) == "function" then
if tonumber(fpslevelbox.Text) > 0 then
setfpscap(tonumber(fpslevelbox.Text))
else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Number must be greater than 0")
end
else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Your exploit does not support this function (setfpscap)")
end
end
end)

end)
-- end of window
end)

function openBG()
local window = Window.new({width = 430, height = 291, name='Backdoor', icon='rbxassetid://0'})
local name  = createnew('TextBox',{
ZIndex = 8;
Text = "";
ClearTextOnFocus = true;
Font = Enum.Font.SourceSansLight;
PlaceholderText = "Player Name (Can be Shortened) / Or All";
TextSize = 20;
MultiLine = false;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0.951,0,0,24);
Position = UDim2.new(0.025,0,0,230);
Parent = window.Content;
})

local Kill = createnew('TextButton',{
