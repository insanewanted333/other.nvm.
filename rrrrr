AddCommand("orbit", function()
for i,Targ in pairs(GetPlayer(args[2])) do
Target = Players[Targ]
    local root = getRootp(LclPlayer.Character)
    local humanoid = LclPlayer.Character:FindFirstChildWhichIsA("Humanoid")
    if Target and Target.Character and getRootp(Target.Character) and root and humanoid then
        local rotation = 0
        local speed = tonumber(args[3]) or 0.2
        local distance = tonumber(args[4]) or 6
        orbit1 = RunService.Heartbeat:Connect(function()
            pcall(function()
                rotation = rotation + speed
                root.CFrame = CFrame.new(getRootp(Target.Character).Position) * CFrame.Angles(0, math.rad(rotation), 0) * CFrame.new(distance, 0, 0)
            end)
        end)
        orbit2 = RunService.RenderStepped:Connect(function()
            pcall(function()
                root.CFrame = CFrame.new(root.Position, getRootp(Target.Character).Position)
            end)
        end)
        orbit3 = humanoid.Died:Connect(function() execCmd("unorbit") end)
        orbit4 = humanoid.Seated:Connect(function(value) if value then execCmd("unorbit") end end)
        
    end
end
end)

AddCommand("unorbit", function()
    if orbit1 then orbit1:Disconnect() end
    if orbit2 then orbit2:Disconnect() end
    if orbit3 then orbit3:Disconnect() end
    if orbit4 then orbit4:Disconnect() end
end)

