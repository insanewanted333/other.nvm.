notviewf = true
viewing = nil
Viewplr.MouseButton1Click:Connect(function()
if notviewf == true then
notviewf = false
Viewplr.Text = "Unview"
local Player = SelPlr
		if viewDied then
			viewDied:Disconnect()
			viewChanged:Disconnect()
		end
		viewing = Player
		workspace.CurrentCamera.CameraSubject = viewing.Character
		local function viewDiedFunc()
			repeat wait() until Player.Character ~= nil and getRootp(Player.Character)
			workspace.CurrentCamera.CameraSubject = viewing.Character
		end
		viewDied = Player.CharacterAdded:Connect(viewDiedFunc)
		local function viewChangedFunc()
			workspace.CurrentCamera.CameraSubject = viewing.Character
		end
		viewChanged = workspace.CurrentCamera:GetPropertyChangedSignal("CameraSubject"):Connect(viewChangedFunc)
else
notviewf = true
Viewplr.Text = "View"
viewing = nil
viewDied:Disconnect()
viewChanged:Disconnect()
	workspace.CurrentCamera.CameraSubject = LclPlayer.Character
end
end)

function updcontent()
if SelPlr then
local con = Players:GetUserThumbnailAsync(SelPlr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
PlrIcon.Image = con
PlrName.Text = SelPlr.Name
Plrid.Text = "ID: " .. SelPlr.UserId
local ag = tostring(SelPlr.Team)
Plrteam.Text = "Team: " .. ag
PlrAge.Text = "Age: " .. SelPlr.AccountAge
notviewf = true
Viewplr.Text = "View"
end
end

updcontent()

function updlist()
for i,v in pairs(PlrsHolder:GetDescendants()) do
v:Destroy()
end

task.wait()

for i,v in pairs(Players:GetPlayers()) do
local PlrBtn = createnew('TextButton',{
ZIndex = 8;
Text = v.Name .. "\n[" .. v.DisplayName .. "]";
Font = Enum.Font.SourceSansLight;
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(1,0,0,24);
Position = UDim2.new(0.001,0,0,0+prevp);
Parent = PlrsHolder;
})

PlrBtn.MouseButton1Click:Connect(function()
SelPlr = Players:FindFirstChild(v.Name)
print(SelPlr)
updcontent()
end)
task.wait()
prevp = prevp + 30
end
end

updlist()
Players.PlayerAdded:Connect(function(player)
prevp = 0
updlist()
end)

Players.PlayerRemoving:Connect(function(player)
prevp = 0
updlist()
end)



end)


tabHolder.Scripts.MouseButton1Click:Connect(function()
local windo = Window.new({width = 250, height = 310, name='Scripts', icon='rbxassetid://18900459968'})

function RunLS(str)
loadstring(game:HttpGet(str), true)()
Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Success", "Script Successfully ran.")
end

local SearchE = createnew('TextBox',{ClearTextOnFocus=true,Font=Enum.Font.SourceSansLight,FontSize=Enum.FontSize.Size24,MultiLine=false,Text='Search Scripts',TextColor3=Color3.fromRGB(255,255,255), PlaceholderText='', PlaceholderColor3=Color3.fromRGB(0.7, 0.7, 0.7),TextScaled=false,TextSize=22,TextStrokeColor3=Color3.fromRGB(0, 0, 0),TextStrokeTransparency=1,TextTransparency=0,TextWrapped=true,TextXAlignment=Enum.TextXAlignment.Center,TextYAlignment=Enum.TextYAlignment.Bottom,Active=true,AnchorPoint=Vector2.new(0, 0),BackgroundColor3=Color3.fromRGB(255,255,255),BackgroundTransparency=1,BorderColor3=Color3.fromRGB(0.105882, 0.164706, 0.207843),BorderSizePixel=1,ClipsDescendants=false,Draggable=false,Position=UDim2.new(0.025, 0, 0, 0),Rotation=0,Selectable=true,Size=UDim2.new(0.95, 0,0.1, 0),SizeConstraint=Enum.SizeConstraint.RelativeXY,Visible=true,ZIndex=9,Name='Searchs',Parent = windo.Content})
materializet(SearchE, Color3.fromRGB(255,255,255), "Search Scripts")

