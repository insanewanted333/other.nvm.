Blackout.BackgroundTransparency = 0.5

MsgBox.Parent = Container
MsgBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
MsgBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
MsgBox.BorderSizePixel = 0
MsgBox.Position = UDim2.new(0.257968187, 0, 0.234126955, 0)
MsgBox.Size = UDim2.new(0.482470155, 0, 0.528571427, 0)

Warning.Parent = MsgBox
Warning.Size = UDim2.new(0, 121, 0, 121)
Warning.BackgroundTransparency = 1
Warning.Image = "rbxassetid://75092996559960"
Warning.ImageTransparency = 0
Warning.Position = UDim2.new(0.01,0,0,10)
Warning.BackgroundColor3 = Color3.fromRGB(200,200,200)

Msg.Parent = MsgBox
Msg.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
