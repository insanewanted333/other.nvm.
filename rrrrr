AddCommand("box", function()
for i, v in pairs(GetPlayer(args[2])) do
		for i, v in pairs(Players[v].Character:GetChildren()) do
			if v:IsA("Accessory") then
				runinst(v)
			end
		end
		for i, v in pairs(Players[v].Character:GetChildren()) do
			if v:IsA("CharacterMesh") then
				runinst(v)
			end
		end
		runinst(Players[v].Character:FindFirstChildOfClass("Pants"))
		runinst(Players[v].Character:FindFirstChildOfClass("Shirt"))
		runinst(Players[v].Character:FindFirstChildOfClass("ShirtGraphic"))
		runinst(Players[v].Character["Left Arm"])
		runinst(Players[v].Character["Left Leg"])
		runinst(Players[v].Character["Right Arm"])
		runinst(Players[v].Character["Right Leg"])
		runinst(Players[v].Character.Head:FindFirstChildOfClass("SpecialMesh"))
		runinst(Players[v].Character.Head:FindFirstChildOfClass("Decal"))
	
end
end)

