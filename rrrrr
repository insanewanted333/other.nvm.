Parent = window.Content;
})

local FakeMsg  = createnew('TextBox',{
ZIndex = 8;
Text = "";
ClearTextOnFocus = false;
Font = Enum.Font.SourceSansLight;
PlaceholderText = "Fake Message";
TextSize = 20;
MultiLine = false;
BackgroundColor3 = Color3.new(1,1,1);
