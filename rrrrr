coroutine.wrap(function() --Put your function if you want or
--Put the loop and your code
local gradient = grade
local ts = cloneref(game:GetService("TweenService"))
local ti = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
local offset1 = {Offset = Vector2.new(.75, 0)}
local create = ts:Create(gradient, ti, offset1)
local startingPos = Vector2.new(-1, 0) 
local addWait = 0

local function animate()
	create:Play()
	create.Completed:Wait() --wait for the tween to stop
	gradient.Offset = startingPos --resets offset
	create:Play() -- repeats anim
	create.Completed:Wait()-- waits for the tween to stop
	gradient.Offset = startingPos
	wait(addWait) --waits some time for the tween to stop
	animate() -- calls itself (for looping)
end
animate() -- calls the all funcions!!
end)()

-- SAVE FILE

function iswritefile()
	if writefile then
		return true
	end
end

defaultsettings = {
prefix = ':';
autobackdoorscan = false;
GuiSnappingK = false;
GuiDraggingK = false;
DBFMenu = false;
ToggleKeyCmd = 'BackSlash';
KToggleKey = 'LeftControl';
KeepWindowsOpen = false;
DelScan = false;
MCRED = 0;
MCGREEN = 0;
MCBLUE = 0;
KblurLevel = 25;
LibScripts = {};
KBlur = true;
PastebinENGINE = false;
ScriptBLOXENGINE = true;
FastBoot = false;
SmartLib = false;
KCBar = false;
}

defaults = HttpService:JSONEncode(defaultsettings)
nosaves = false
useFactorySettings = function()
prefix = ':';
autobackdoorscan = false;
GuiSnappingK = false;
GuiDraggingK = false;
DBFMenu = false;
ToggleKeyCmd = 'BackSlash';
KToggleKey = 'LeftControl';
KeepWindowsOpen = false;
DelScan = false;
