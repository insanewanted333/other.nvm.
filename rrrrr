for i,v in next, LibScripts do
if v[1] == name then
table.remove(LibScripts, i)
data:Destroy()
Notify({"rbxassetid://1375852124", Color3.fromRGB(0,0,0)}, "Deleted Script", "Script \""..name.."\" has been deleted from your library.")
return
end
end

end
end)
end
end
