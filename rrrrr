AddCommand("jobid", function()
	local jobId = 'Roblox.GameLauncher.joinGameInstance('..PlaceId..', "'..JobId..'")'
	copyclipboard(jobId)
Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "JobId", "Job Id has been copied to your clipboard")
end)

AddCommand("autorejoin", function()
GuiService.ErrorMessageChanged:Connect(function()
		runCmd("rejoin")
	end)
	Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Automatic Rejoins", "You will automatically rejoin when kicked/error message popup\nAuto Rejoin Enabled")
end)

AddCommand("shop", function()
if httprequest then
        local servers = {}
        local req = httprequest({Url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Desc&limit=100&excludeFullGames=true", PlaceId)})
        local body = HttpService:JSONDecode(req.Body)

        if body and body.data then
            for i, v in next, body.data do
                if type(v) == "table" and tonumber(v.playing) and tonumber(v.maxPlayers) and v.playing < v.maxPlayers and v.id ~= JobId then
                    table.insert(servers, 1, v.id)
                end
            end
        end

        if #servers > 0 then
            TeleportService:TeleportToPlaceInstance(PlaceId, servers[math.random(1, #servers)], Players.LocalPlayer)
        else
            return Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Servers", "Couldn't find a server.")
        end
    else
        Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Unsupported Exploit", "Your exploit does not support this function [httprequest]")
    end
end)
Addally("shop","serverhop")

AddCommand("exit", function()
game:Shutdown()
end)
Addally("exit","quit")

local coreGuiTypeNames = {
	-- predefined aliases
	["inventory"] = Enum.CoreGuiType.Backpack,
	["leaderboard"] = Enum.CoreGuiType.PlayerList,
	["emotes"] = Enum.CoreGuiType.EmotesMenu
}

-- Load the full list of enums
for _, enumItem in ipairs(Enum.CoreGuiType:GetEnumItems()) do
	coreGuiTypeNames[enumItem.Name:lower()] = enumItem
end


AddCommand("enable", function()
	local input = args[2] and args[2]:lower()
	if input then
		if input == "reset" then
			StarterGui:SetCore("ResetButtonCallback", true)
		else
			local coreGuiType = coreGuiTypeNames[input]
			if coreGuiType then
				StarterGui:SetCoreGuiEnabled(coreGuiType, true)
			end
		end
	end
end)

AddCommand("disable", function()
	local input = args[2] and args[2]:lower()
	if input then
		if input == "reset" then
			StarterGui:SetCore("ResetButtonCallback", false)
		else
			local coreGuiType = coreGuiTypeNames[input]
			if coreGuiType then
				StarterGui:SetCoreGuiEnabled(coreGuiType, false)
			end
		end
	end
end)

function FindInTable(tbl,val)
	if tbl == nil then return false end
	for _,v in pairs(tbl) do
		if v == val then return true end
	end 
	return false
end

local invisGUIS = {}
AddCommand("showguis", function()
local function doit()
	for i,v in pairs(LclPlayer:FindFirstChildWhichIsA("PlayerGui"):GetDescendants()) do
		if (v:IsA("Frame") or v:IsA("ImageLabel") or v:IsA("ScrollingFrame")) and not v.Visible then
			v.Visible = true
			if not FindInTable(invisGUIS,v) then
				table.insert(invisGUIS,v)
			end
		end
	end
end
doit()
end)

AddCommand("unshowguis", function()
	for i,v in pairs(invisGUIS) do
		v.Visible = false
	end
	invisGUIS = {}
end)

local hiddenGUIS = {}
AddCommand("hideguis", function()
	for i,v in pairs(LclPlayer:FindFirstChildWhichIsA("PlayerGui"):GetDescendants()) do
		if (v:IsA("Frame") or v:IsA("ImageLabel") or v:IsA("ScrollingFrame")) and v.Visible then
			v.Visible = false
			if not FindInTable(hiddenGUIS,v) then
				table.insert(hiddenGUIS,v)
			end
		end
	end
end)

AddCommand("unhideguis", function()
	for i,v in pairs(hiddenGUIS) do
		v.Visible = true
	end
	hiddenGUIS = {}
end)

