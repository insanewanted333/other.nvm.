	end
end)

AddCommand("savegame", function()
    if saveinstance then
    defaultnotify("Saving game","This might take a while")
        saveinstance()
        defaultnotify("Game Saved", "The game is saves to your exploits workspace folder")
    else
        defaultnotify("Unsupported", "Your exploit does not support this function [saveinstance]")
    end
end)
Addally("savegame","saveplace")

AddCommand("clearerror", function()
	GuiService:ClearError()
end)
Addally("clearerror","clearerrors")

AddCommand("antikick", function()
	if not hookmetamethod then 
		return defaultnotify('Unsupported','Your exploit does not support this function [hookmetamethod]')
	end
	local LocalPlayer = Players.LocalPlayer
	local oldhmmi
	local oldhmmnc
	oldhmmi = hookmetamethod(game, "__index", function(self, method)
		if self == LocalPlayer and method:lower() == "kick" then
			return error("Expected ':' not '.' calling member function Kick", 2)
		end
		return oldhmmi(self, method)
	end)
	oldhmmnc = hookmetamethod(game, "__namecall", function(self, ...)
		if self == LocalPlayer and getnamecallmethod():lower() == "kick" then
			return
		end
		return oldhmmnc(self, ...)
	end)

	defaultnotify('Anti-Kick','Anti Kick is now on')
end)

AddCommand("antiteleport", function()
	if not hookmetamethod then 
		return defaultnotify('Unsupported','Your exploit does not support this function [hookmetamethod]')
	end
	local TeleportService = TeleportService
	local oldhmmi
	local oldhmmnc
	oldhmmi = hookmetamethod(game, "__index", function(self, method)
		if self == TeleportService then
			if method:lower() == "teleport" then
				return error("Expected ':' not '.' calling member function Kick", 2)
			elseif method == "TeleportToPlaceInstance" then
				return error("Expected ':' not '.' calling member function TeleportToPlaceInstance", 2)
			end
		end
		return oldhmmi(self, method)
	end)
	oldhmmnc = hookmetamethod(game, "__namecall", function(self, ...)
		if self == TeleportService and getnamecallmethod():lower() == "teleport" or getnamecallmethod() == "TeleportToPlaceInstance" then
			return
		end
		return oldhmmnc(self, ...)
	end)

	defaultnotify('Anti Teleport','Anti Teleport is now on')
	end)
	Addally("antiteleport","antitp")

AddCommand("canceltp", function()
	TeleportService:TeleportCancel()
end)
Addally("canceltp","cancelteleport")

AddCommand("vol" ,function()
	local level = args[2]/10
	UserSettings():GetService("UserGameSettings").MasterVolume = level
end)
addally("vol","volume")

AddCommand("setfpscap", function()
	if setfpscap and type(setfpscap) == "function" then
		local num = args[2] or 1e6
		if num == 'none' then
			return setfpscap(1e6)
		elseif num > 0 then
			return setfpscap(num)
		else
			return defaultnotify('error', "Number must be above 0")
		end
	else
		return defaultnotify('Unsupported', 'Your exploit does not support this function [setfpscap]')
	end
end)
addally("setfpscap","maxfps","fpscap","fpslimit")

AddCommand("fov", function()
	local fov = args[2] or 70
	if isNumber(fov) then
		workspace.CurrentCamera.FieldOfView = fov
	end
end)
addally("fov","fieldofview")

local loopgoto = nil
AddCommand("loopgoto", function()
	for i,v in pairs(GetPlayer(args[2]))do
		loopgoto = nil
		if LclPlayer.Character:FindFirstChildOfClass('Humanoid') and LclPlayer.Character:FindFirstChildOfClass('Humanoid').SeatPart then
			LclPlayer.Character:FindFirstChildOfClass('Humanoid').Sit = false
			wait(.1)
		end
		loopgoto = Players[v]
		local distance = 3
		if args[3] and isNumber(args[3]) then
			distance = args[3]
		end
		local lDelay = 0
		if args[4] and isNumber(args[4]) then
			lDelay = args[4]
		end
		repeat
			if Players:FindFirstChild(v) then
				if Players[v].Character ~= nil then
					getRootp(LclPlayer.Character).CFrame = getRootp(Players[v].Character).CFrame + Vector3.new(distance,1,0)
				end
				wait(lDelay)
			else
				loopgoto = nil
			end
		until loopgoto ~= Players[v]
	end
end)

AddCommand("unloopgoto", function()
loopgoto = nil
end)


AddCommand("pm", function()
for i,v in pairs(GetPlayer(args[2]))do
		task.spawn(function()
			local plrName = Players[v].Name
			local pmstring = tostring(args[3])
			chatMessage("/w "..plrName.." "..pmstring)
		end)
	end
	end)
addally("pm","whisper")

local HumanModCons = {}
AddCommand("autojump", function()
	local Char = LclPlayer.Character
	local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")
	local function autoJump()
		if Char and Human then
			local check1 = workspace:FindPartOnRay(Ray.new(Human.RootPart.Position-Vector3.new(0,1.5,0), Human.RootPart.CFrame.lookVector*3), Human.Parent)
			local check2 = workspace:FindPartOnRay(Ray.new(Human.RootPart.Position+Vector3.new(0,1.5,0), Human.RootPart.CFrame.lookVector*3), Human.Parent)
			if check1 or check2 then
				Human.Jump = true
			end
		end
	end
	autoJump()
	HumanModCons.ajLoop = (HumanModCons.ajLoop and HumanModCons.ajLoop:Disconnect() and false) or RunService.RenderStepped:Connect(autoJump)
	HumanModCons.ajCA = (HumanModCons.ajCA and HumanModCons.ajCA:Disconnect() and false) or LclPlayer.CharacterAdded:Connect(function(nChar)
		Char, Human = nChar, nChar:WaitForChild("Humanoid")
		autoJump()
		HumanModCons.ajLoop = (HumanModCons.ajLoop and HumanModCons.ajLoop:Disconnect() and false) or RunService.RenderStepped:Connect(autoJump)
	end)
end)

AddCommand("unautojump", function()
	HumanModCons.ajLoop = (HumanModCons.ajLoop and HumanModCons.ajLoop:Disconnect() and false) or nil
	HumanModCons.ajCA = (HumanModCons.ajCA and HumanModCons.ajCA:Disconnect() and false) or nil
end)

AddCommand("fixcam", function()
	runCmd('unview')
	workspace.CurrentCamera:remove()
	wait(.1)
	repeat wait() until LclPlayer.Character ~= nil
	workspace.CurrentCamera.CameraSubject = speaker.Character:FindFirstChildWhichIsA('Humanoid')
	workspace.CurrentCamera.CameraType = "Custom"
	LclPlayer.CameraMinZoomDistance = 0.5
	LclPlayer.CameraMaxZoomDistance = 400
	LclPlayer.CameraMode = "Classic"
	LclPlayer.Character.Head.Anchored = false
end)

AddCommand("firstp", function()
	LclPlayer.CameraMode = "LockFirstPerson"
end)

AddCommand("thirdp", function()
	LclPlayer.CameraMode = "Classic"
end)

AddCommand("noclipcam", function()
	local sc = (debug and debug.setconstant) or setconstant
	local gc = (debug and debug.getconstants) or getconstants
	if not sc or not getgc or not gc then
		return
	end
	local pop = LclPlayer.PlayerScripts.PlayerModule.CameraModule.ZoomController.Popper
	for _, v in pairs(getgc()) do
		if type(v) == 'function' and getfenv(v).script == pop then
			for i, v1 in pairs(gc(v)) do
				if tonumber(v1) == .25 then
					sc(v, i, 0)
				elseif tonumber(v1) == 0 then
					sc(v, i, .25)
				end
			end
		end
	end
end)

AddCommand("maxzoom", function()
	LclPlayer.CameraMaxZoomDistance = args[2]
end)

AddCommand("minzoom", function()
	LclPlayer.CameraMinZoomDistance = args[2]
end)

AddCommand("unlockws", function()
	for i,v in pairs(workspace:GetDescendants()) do
		if v:IsA("BasePart") then
			v.Locked = false
		end
	end
end)

AddCommand("lockws", function()
	for i,v in pairs(workspace:GetDescendants()) do
		if v:IsA("BasePart") then
			v.Locked = true
		end
	end
end)

