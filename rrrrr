chatMessage(" "..string.rep("ৌ", 199))
end)

local Reset = createnew('TextButton',{
ZIndex = 8;
Text = "Reset Filter";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .83;
TextColor3 = Color3.fromRGB(255,255,255);
Size = UDim2.new(.95,0,0,24);
Font = Enum.Font.SourceSansLight;
Position = UDim2.new(0.025,0,0,270);
Parent = window.Content;
})
Reset.MouseButton1Click:Connect(function()
local PlayerService = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TextChatService = game:GetService("TextChatService")

local IsLegacy = (TextChatService.ChatVersion == Enum.ChatVersion.LegacyChatService)
local ChatRemote = ReplicatedStorage:FindFirstChild("SayMessageRequest", true)
local Channel = not IsLegacy and TextChatService.TextChannels.RBXGeneral

local Chat = function(Message)
    if IsLegacy then
        ChatRemote:FireServer(Message, "All")
    else
        Channel:SendAsync(Message)
    end
end

local Fake = function(Message)
    if IsLegacy then
        PlayerService:Chat(Message)
    else
        Channel:SendAsync(("/e %s"):format(Message))
    end
end

local chars = {}
for i=97,122 do chars[#chars+1]=string.char(i) end
for i=65,90 do chars[#chars+1]=string.char(i) end

local RNG = function(length)
    local str = ""
    for i = 1, length do
        str = str .. chars[math.random(#chars)]
    end
    return str
end

local ResetFilter = function()
    for i = 1, 10 do
        local GUID = RNG(i)
        local Filler = "i love asians so much"
        local Reset = ("%s %s"):format(GUID, Filler)
        task.spawn(function()
            Fake(Reset)
        end)
    end
end
ResetFilter()
end)
end

function openspam()
local window = Window.new({width = 300, height = 120, name='Audio Spammer', icon='rbxassetid://109603048517667'})

local delay  = createnew('TextBox',{
ZIndex = 8;
Text = "";
ClearTextOnFocus = false;
Font = Enum.Font.SourceSansLight;
PlaceholderText = "Delay";
TextSize = 20;
MultiLine = false;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0.95,0,0,24);
Position = UDim2.new(0.025,0,0,0);
Parent = window.Content;
})
delay.Changed:connect(function()
delay.Text = delay.Text:gsub("%D","")
end)

local Stop = createnew('TextButton',{
ZIndex = 8;
Text = "Stop";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .83;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(.95,0,0,24);
Font = Enum.Font.SourceSansLight;
Position = UDim2.new(0.025,0,0,30);
Parent = window.Content;
})

local start = createnew('TextButton',{
ZIndex = 8;
Text = "Start";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .83;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(.95,0,0,24);
Font = Enum.Font.SourceSansLight;
Position = UDim2.new(0.025,0,0,60);
Parent = window.Content;
})

local On = false

start.MouseButton1Click:Connect(function()
if cloneref(game:GetService("SoundService")).RespectFilteringEnabled == false then
On = true
 while On == true do
 if delay.Text == "" then
task.wait()
else
task.wait(delay.Text)
end
  for _, sound in next, workspace:GetDescendants() do
   if sound:IsA("Sound") then
    sound:Play()
   end
  end
 end
else
defaultnotify("Unsupported Game","Respect Filtering Enabled is turned On")
end
end)

Stop.MouseButton1Click:Connect(function()
On = false
end)
end

