Position = UDim2.new(0.025,0,0,30);
Parent = window.Content;
})


local statusman = createnew('TextButton',{
ZIndex = 8;
Text = "Status: Working";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .83;
TextColor3 = Color3.new(1,1,1);
