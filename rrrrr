AddCommand("r15", function()
AvatarEditorService:PromptSaveAvatar(game.Players.LocalPlayer.Character.Humanoid.HumanoidDescription,Enum.HumanoidRigType.R15)
Notify("Press Allow And Wait For A While")
local result = AvatarEditorService.PromptSaveAvatarCompleted:Wait()
if result == Enum.AvatarPromptResult.Success
then
LclPlayer.Character.Humanoid.Health = 0
else
warn("error on switching")
end
end)

AddCommand("r6", function()
AvatarEditorService:PromptSaveAvatar(game.Players.LocalPlayer.Character.Humanoid.HumanoidDescription,Enum.HumanoidRigType.R6)
Notify("Press Allow And Wait For A While")
local result = AvatarEditorService.PromptSaveAvatarCompleted:Wait()
if result == Enum.AvatarPromptResult.Success
then
LclPlayer.Character.Humanoid.Health = 0
else
warn("error on switching")
end
end)

AddCommand("fullbright", function()
Lighting.Brightness = 2
	Lighting.ClockTime = 14
	Lighting.FogEnd = 100000
	Lighting.GlobalShadows = false
	Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
end)

AddCommand("day", function()
Lighting.ClockTime = 14
end)

AddCommand("night", function()
Lighting.ClockTime = 0
end)


AddCommand("fb", function()
Lighting.Brightness = 2
	Lighting.ClockTime = 14
	Lighting.FogEnd = 100000
	Lighting.GlobalShadows = false
	Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
end)

AddCommand("nofog", function()
Lighting.FogEnd = 100000
	for i,v in pairs(Lighting:GetDescendants()) do
		if v:IsA("Atmosphere") then
			v:Destroy()
		end
	end
end)

AddCommand("globalshadows", function()
Lighting.GlobalShadows = true
end)

AddCommand("noglobalshadows", function()
Lighting.GlobalShadows = false
end)

origsettings = {abt = Lighting.Ambient, oabt = Lighting.OutdoorAmbient, brt = Lighting.Brightness, time = Lighting.ClockTime, fe = Lighting.FogEnd, fs = Lighting.FogStart, gs = Lighting.GlobalShadows}
AddCommand("restorelighting", function()
Lighting.Ambient = origsettings.abt
	Lighting.OutdoorAmbient = origsettings.oabt
	Lighting.Brightness = origsettings.brt
	Lighting.ClockTime = origsettings.time
	Lighting.FogEnd = origsettings.fe
	Lighting.FogStart = origsettings.fs
	Lighting.GlobalShadows = origsettings.gs
end)

AddCommand("lay", function()
local Human = kplr.Character and kplr.Character:FindFirstChildOfClass('Humanoid')
	if not Human then
		return
	end
	Human.Sit = true
	task.wait(.1)
	Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
	for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
		v:Stop()
	end
end)

swimming = false
local oldgrav = workspace.Gravity
local swimbeat = nil
AddCommand("swim", function()
if not swimming and kplr and kplr.Character and kplr.Character:FindFirstChildWhichIsA("Humanoid") then
		oldgrav = workspace.Gravity
		workspace.Gravity = 0
		local swimDied = function()
			workspace.Gravity = oldgrav
			swimming = false
		end
		local Humanoid = kplr.Character:FindFirstChildWhichIsA("Humanoid")
		gravReset = Humanoid.Died:Connect(swimDied)
		local enums = Enum.HumanoidStateType:GetEnumItems()
		table.remove(enums, table.find(enums, Enum.HumanoidStateType.None))
		for i, v in pairs(enums) do
			Humanoid:SetStateEnabled(v, false)
		end
		Humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
		swimbeat = RunService.Heartbeat:Connect(function()
			pcall(function()
				kplr.Character.HumanoidRootPart.Velocity = ((Humanoid.MoveDirection ~= Vector3.new() or UserInputService:IsKeyDown(Enum.KeyCode.Space)) and kplr.Character.HumanoidRootPart.Velocity or Vector3.new())
			end)
		end)
		swimming = true
	end
end)

AddCommand("unswim", function()
if kplr and kplr.Character and kplr.Character:FindFirstChildWhichIsA("Humanoid") then
		workspace.Gravity = oldgrav
		swimming = false
		if gravReset then
			gravReset:Disconnect()
		end
		if swimbeat ~= nil then
			swimbeat:Disconnect()
			swimbeat = nil
		end
		local Humanoid = kplr.Character:FindFirstChildWhichIsA("Humanoid")
		local enums = Enum.HumanoidStateType:GetEnumItems()
		table.remove(enums, table.find(enums, Enum.HumanoidStateType.None))
		for i, v in pairs(enums) do
			Humanoid:SetStateEnabled(v, true)
		end
	end
end)

CFspeed = 50
AddCommand("cfly", function()
--Full credit to peyton#9148
	kplr.Character:FindFirstChildOfClass('Humanoid').PlatformStand = true
	local Head = kplr.Character:WaitForChild("Head")
	Head.Anchored = true
	if CFloop then CFloop:Disconnect() end
	CFloop = RunService.Heartbeat:Connect(function(deltaTime)
		local moveDirection = kplr.Character:FindFirstChildOfClass('Humanoid').MoveDirection * (CFspeed * deltaTime)
		local headCFrame = Head.CFrame
		local cameraCFrame = workspace.CurrentCamera.CFrame
		local cameraOffset = headCFrame:ToObjectSpace(cameraCFrame).Position
		cameraCFrame = cameraCFrame * CFrame.new(-cameraOffset.X, -cameraOffset.Y, -cameraOffset.Z + 1)
		local cameraPosition = cameraCFrame.Position
		local headPosition = headCFrame.Position

		local objectSpaceVelocity = CFrame.new(cameraPosition, Vector3.new(headPosition.X, cameraPosition.Y, headPosition.Z)):VectorToObjectSpace(moveDirection)
		Head.CFrame = CFrame.new(headPosition) * (cameraCFrame - cameraPosition) * CFrame.new(objectSpaceVelocity)
	end)
end)

AddCommand("uncfly", function()
if CFloop then
		CFloop:Disconnect()
kplr.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
		local Head = kplr.Character:WaitForChild("Head")
		Head.Anchored = false
	end
end)

AddCommand("cflyspeed", function()
if isNumber(args[2]) then
		CFspeed = args[2]
	end
end)

viewing = nil
AddCommand("view", function()
for _, v in pairs(GetPlayer(args[2])) do
local Player = Players[v]
		if viewDied then
			viewDied:Disconnect()
			viewChanged:Disconnect()
		end
		viewing = Player
		workspace.CurrentCamera.CameraSubject = viewing.Character
		local function viewDiedFunc()
			repeat wait() until Player.Character ~= nil and getRootp(Player.Character)
			workspace.CurrentCamera.CameraSubject = viewing.Character
		end
		viewDied = Player.CharacterAdded:Connect(viewDiedFunc)
		local function viewChangedFunc()
			workspace.CurrentCamera.CameraSubject = viewing.Character
		end
		viewChanged = workspace.CurrentCamera:GetPropertyChangedSignal("CameraSubject"):Connect(viewChangedFunc)
	end
end)

AddCommand("unview", function()
		viewing = nil
		viewDied:Disconnect()
		viewChanged:Disconnect()
	workspace.CurrentCamera.CameraSubject = kplr.Character
end)

AddCommand("killall", function()
if not RCDEnabled then
     local player_table = Players:GetPlayers()
        local toolsInBackpack = 0
        local toolsEquipped = 0
        local players = {}
        local tools = {}
        
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                toolsInBackpack = toolsInBackpack + 1
        end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.ClassName == "Tool" then
                        toolsEquipped = toolsEquipped + 1
                end
        end
        local total_tools = toolsInBackpack + toolsEquipped
        print(#player_table.." players")
        
                for i,v in next, player_table do
                        if v.Character.Humanoid.Sit ~= true and v ~= LclPlayer and v.Character.Humanoid.Health ~= 0 then
                                table.insert(players, v)
                        end
                end
                local newHum = game.Players.LocalPlayer.Character.Humanoid:Clone()
                newHum.Parent = game.Players.LocalPlayer.Character
                game.Players.LocalPlayer.Character.Humanoid:Destroy()
                newHum:ChangeState(15)
                for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
                        if v:IsA'Tool' then
                                v.Parent = game.Players.LocalPlayer.Character
                        end
                end
                wait(.1)
                for i,v in next, game.Players.LocalPlayer.Character:GetChildren() do
                        if v:IsA'Tool' then
                                table.insert(tools, v)
                        end
                end
                local currentTargets = {}
                for i, tool in next, tools do
tool.Handle.Massless = true
                        tool.Grip = CFrame.new()
                        tool.Grip = tool.Handle.CFrame:ToObjectSpace(players[i].Character.Head.CFrame):Inverse()
                end
                local players = {}
end
end)

-- all new

function AddCmds()
table.insert(list, 1, "-- BACKDOOR COMMANDS --")
table.insert(list, 2, "[1] Kill {plr} - kills player")
table.insert(list, 3, "[2] Naked {plr} - makes player naked")
table.insert(list, 4, "[3] Faceless {plr} - makes player faceless")
table.insert(list, 5, "[4] Ban {plr} - bans player")
table.insert(list, 6, "[5] Unban {plr} - unbans player")
table.insert(list, 7, "[6] Kick {plr} - kicks player")
table.insert(list, 8, "[7] Nora {plr} - Removes players right arm")
table.insert(list, 9, "[8] Nola {plr} - Removes players left arm")
table.insert(list, 10, "[9] Norl {plr} - Removes players right leg")
table.insert(list, 11, "[10] Noll {plr} - Removes players left leg")
table.insert(list, 12, "[11] Blockchar {plr} - Makes players character look blocky")
table.insert(list, 13, "[12] Noanim {plr} - Removes players animation")
table.insert(list, 14, "[13] Punish {plr} - Punishes player (NOT UNDOABLE)")
table.insert(list, 15, "[14] Shirtless {plr} - Removes players shirt")
table.insert(list, 16, "[15] Tshirtless {plr} - Removes players tshirt")
table.insert(list, 17, "[16] Box {plr} - Makes player a box")
table.insert(list, 18, "[17] Notools {plr} - Removes all tools from players inventory")
table.insert(list, 19, "[18] Bald {plr} - Makes player bald")
table.insert(list, 20, "[19] Sink {plr} - Sinks player")
table.insert(list, 21, "[20] Slock - Locks server")
table.insert(list, 22, "[21] Unslock - Unlocks server")
table.insert(list, 23, "[22] Mute - Mutes Player")
table.insert(list, 24, "-- NORMAL COMMANDS --")

AddCommand("naked", function()
for i,v in pairs(GetPlayer(args[2])) do
local plr = Players[v]
		runinst(plr.Character:FindFirstChildOfClass("Pants"))
		runinst(plr.Character:FindFirstChildOfClass("Shirt"))
		runinst(plr.Character:FindFirstChildOfClass("ShirtGraphic"))
	end
end)

AddCommand("faceless", function()
for i,v in pairs(GetPlayer(args[2])) do
local plr = Players[v]
		runinst(plr.Character.Head:FindFirstChildOfClass("Decal"))
	end
end)
SLock = false

AddCommand("mute", function()
if not modernChat then return defaultnotify("No Support","Game uses legacy chat. Mute UnSupported") end
local names = {}
	for i,v in pairs(GetPlayer(args[2])) do
		table.insert(names, Players[v].Name)
	end
	for i,instance in pairs(TextChatService.TextChannels:GetDescendants()) do
		if not table.find(names, instance.Name) then continue end
		runinst(instance)
	end
end)

local banned = {}
Players.PlayerAdded:connect(function(plr)
	if SLock then
		Destroy(plr)
	end
	if FindInTable(banned, plr.UserId) then
		Destroy(plr)
	end
end)

AddCommand("ban", function()
for i,v in pairs(GetPlayer(args[2])) do
local plr = Players[v]
	table.insert(banned, plr.UserId)
runinst(plr)
end
end)

