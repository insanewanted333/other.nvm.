ScriptBLOXENGINE = true;
FastBoot = false;
SmartLib = false;
KCBar = false;
end

local loadedEventDatak = nil
local jsonAttemptsk = 0
function savesk()
	if iswritefile() and jsonAttemptsk < 50 then
		if pcall(function() readfile("KRUNOX.FE") end) then
			if readfile("KRUNOX.FE") ~= nil then
				local success, response = pcall(function()
					local json = HttpService:JSONDecode(readfile("KRUNOX.FE"))
if json.prefix ~= nil then prefix = json.prefix else prefix = ':' end

if json.autobackdoorscan ~= nil then autobackdoorscan = json.autobackdoorscan else autobackdoorscan = false end

if json.GuiSnappingK ~= nil then GuiSnappingK = json.GuiSnappingK else GuiSnappingK = false end

if json.GuiDraggingK ~= nil then GuiDraggingK = json.GuiDraggingK else GuiDraggingK = false end

if json.DBFMenu ~= nil then DBFMenu = json.DBFMenu else DBFMenu = false end

if json.ToggleKeyCmd ~= nil then ToggleKeyCmd = json.ToggleKeyCmd else ToggleKeyCmd = 'BackSlash' end

if json.KToggleKey ~= nil then KToggleKey = json.KToggleKey else KToggleKey = 'LeftControl' end


if json.KeepWindowsOpen ~= nil then KeepWindowsOpen = json.KeepWindowsOpen else KeepWindowsOpen = false end

if json.DelScan ~= nil then DelScan = json.DelScan else DelScan = false end

if json.MCRED ~= nil then MCRED = json.MCRED else MCRED = 0 end

if json.MCGREEN ~= nil then MCGREEN = json.MCGREEN else MCGREEN = 0 end

if json.MCBLUE ~= nil then MCBLUE = json.MCBLUE else MCBLUE = 0 end

if json.KblurLevel ~= nil then KblurLevel = json.KblurLevel else KblurLevel = 25 end

if json.LibScripts ~= nil then LibScripts = json.LibScripts else LibScripts = {} end

if json.KBlur ~= nil then KBlur = json.KBlur else KBlur = true end

if json.PastebinENGINE ~= nil then PastebinENGINE = json.PastebinENGINE else PastebinENGINE = false end

if json.ScriptBLOXENGINE ~= nil then ScriptBLOXENGINE = json.ScriptBLOXENGINE else ScriptBLOXENGINE = true end

if json.FastBoot ~= nil then FastBoot = json.FastBoot else FastBoot = false end

if json.SmartLib ~= nil then SmartLib = json.SmartLib else SmartLib = false end

if json.KCBar ~= nil then KCBar = json.KCBar else KCBar = false end


					
end)
				if not success then
					jsonAttemptsk = jsonAttemptsk + 1
					warn("Save Json Error:", response)
					warn("Rewriting Save File")
					writefile("KRUNOX.FE", defaults)
					wait()
					savesk()
				end
			else
				writefile("KRUNOX.FE", defaults)
				wait()
				savesk()
			end
		else
			writefile("KRUNOX.FE", defaults)
			wait()
			if pcall(function() readfile("KRUNOX.FE") end) then
				savesk()
			else
				nosaves = true
				useFactorySettings()

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Krunox";
	Text = "Unable to use writefile?";
	Duration = 5;
})
			end
		end
	else
        if jsonAttemptsk >= 50 then
            nosaves = true
            useFactorySettings()

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Krunox";
	Text = "Unable to read saved data";
	Duration = 5;
})
        else
            useFactorySettings()
        end
	end
end

savesk()

function updatesave()
	if nosaves == false and iswritefile() then
		local update = {
prefix = prefix;
autobackdoorscan = autobackdoorscan;
GuiSnappingK = GuiSnappingK;
GuiDraggingK = GuiDraggingK;
DBFMenu = DBFMenu;
ToggleKeyCmd = ToggleKeyCmd;
KToggleKey = KToggleKey;
KeepWindowsOpen = KeepWindowsOpen;
DelScan = DelScan;
MCRED = MCRED;
MCGREEN = MCGREEN;
MCBLUE = MCBLUE;
KblurLevel = blurLevel;
LibScripts = LibScripts;
KBlur = Blur;
PastebinENGINE = PastebinENGINE;
ScriptBLOXENGINE = ScriptBLOXENGINE;
FastBoot = FastBoot;
SmartLib = SmartLib;
KCBar = KCBar;
		}
		writefile("KRUNOX.FE", HttpService:JSONEncode(update))
	end
end

